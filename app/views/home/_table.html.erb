<div class="table">


      
  <div class="row small-up-2 medium-up-3 large-up-4">
    <%# byebug %>
    <% @game.trick.cards.each do |card| %>
      <div class="column column-block">
        <div class="card">
          <div class="card-image float-center">
            <%# byebug %>
            <img src="/assets/cards/<%= card %>.png">
          </div>
          <div class="card-section float-center">
            Trick-Karte
          </div>
        </div>
      </div>
    <% end %>
  </div>

  
<div class="card">
  <div class="card-image float-center">
     <img src="/assets/cards/back.png">
  </div>
  <div class="card-section float-center">Karte von: Mir</div>
</div>

<%= link_to "Neues Spiel", home_create_game_path(players: players, game: game), method: :post %>

</div>
